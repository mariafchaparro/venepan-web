---
import Layout from '../layouts/Layout.astro';
import DarkBanner from '../components/DarkBanner.astro';
import CTASection from "../components/productos/CTASection.astro";
import ProductSection from "../components/productos/ProductSection.astro";

import bannerProductos from "../assets/images/productos/banner-productos.svg";

export const prerender = true;
---

<Layout title="Productos">
    <DarkBanner 
        image={bannerProductos}
        title='Productos Venepan'
        subtitle='Descubre la variedad de productos que Venepan tiene para tu mesa.'
        currentPath='/productos'
    />
    <ProductSection />
    <CTASection />
    
</Layout>