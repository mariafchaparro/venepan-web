---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Banner from '../components/inicio/Banner.astro';
import ProductSection from '../components/inicio/ProductSection.astro';
import AboutSection from '../components/inicio/AboutSection.astro';
import IAHFCSection from '../components/inicio/IAHFCSection.astro';
import ReviewSection from '../components/inicio/ReviewSection.astro';
import FAQSection from '../components/inicio/FAQSection.astro';
import CTASection from '../components/inicio/CTASection.astro';

export const prerender = true;
---

<Layout title="Inicio">
    <Header classicHeader={true} currentPath={'/'}/>
    <Banner />
    <ProductSection />
    <AboutSection />
    <IAHFCSection />
    <ReviewSection />
    <FAQSection />
    <CTASection />
</Layout>
